{
	"extends": "astro/tsconfigs/strict",
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"baseUrl": "./",
		"paths": {
			"@src/*": ["src/*"],
			"@components/*": ["src/components/*"],
			"@icons/*": ["src/components/Icons/*"],
			"@layouts/*": ["src/layouts/*"],
			"@pages/*": ["src/pages/*"],
			"@data/*": ["src/data/*"],
			"@config/*": ["src/config/*"],
			"@type/*": ["src/data/types/*"]
		}
	}
}
